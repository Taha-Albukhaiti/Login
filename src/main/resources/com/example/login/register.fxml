<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login.RegisterController">
   <children>
      <AnchorPane prefHeight="95.0" prefWidth="520.0" style="-fx-background-color: #050A30#050A30;">
         <children>
            <Label layoutX="119.0" layoutY="36.0" text="User Registeration" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ImageView fx:id="shieldimageView" fitHeight="68.0" fitWidth="90.0" layoutX="9.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true" />
         </children></AnchorPane>
      <Label layoutX="80.0" layoutY="130.0" text="Firstname">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="firstnameTextField" layoutX="80.0" layoutY="155.0" prefHeight="25.0" prefWidth="370.0" promptText="Firstname" />
      <Label layoutX="80.0" layoutY="190.0" text="Lastname">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="lastnameTextField" layoutX="80.0" layoutY="215.0" prefHeight="25.0" prefWidth="370.0" promptText="Lastname" />
      <Label layoutX="80.0" layoutY="250.0" text="Username">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="80.0" layoutY="275.0" prefHeight="25.0" prefWidth="370.0" promptText="Username" />
      <Label layoutX="80.0" layoutY="430.0" text="Confirm Password">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="80.0" layoutY="370.0" prefHeight="21.0" prefWidth="68.0" text="Password">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <PasswordField fx:id="coniformPasswordField" layoutX="80.0" layoutY="455.0" prefHeight="25.0" prefWidth="370.0" promptText="Coniform Password" />
      <PasswordField fx:id="setPasswordField" layoutX="80.0" layoutY="395.0" prefHeight="25.0" prefWidth="370.0" promptText="Password" />
      <Label layoutX="80.0" layoutY="310.0" prefHeight="21.0" prefWidth="32.0" text="Email">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="emailTextField" layoutX="80.0" layoutY="335.0" prefHeight="25.0" prefWidth="370.0" promptText="Email" />
      <Button fx:id="registerButton" layoutX="80.0" layoutY="500.0" mnemonicParsing="false" onAction="#registerButtonOnAction" prefWidth="370.0" style="-fx-background-color: #050A30#050A30 #050A30#050A30;" text="Register" textFill="WHITE" />
      <Button fx:id="closeButton" layoutX="80.0" layoutY="540.0" mnemonicParsing="false" onAction="#closeButtonOnAction" prefWidth="370.0" style="-fx-background-color: #050A30#050A30 #050A30#050A30;" text="Close" textFill="WHITE" />
      <Label fx:id="registrationMessageLabel" alignment="CENTER" layoutX="14.0" layoutY="113.0" prefHeight="17.0" prefWidth="493.0" textFill="#050a30" />
      <Label fx:id="confirmPasswordLabel" alignment="CENTER" layoutX="250.0" layoutY="431.0" prefHeight="17.0" prefWidth="200.0" textFill="#fc8585" />
      <Label fx:id="firstnameLabel" alignment="CENTER" layoutX="250.0" layoutY="131.0" prefHeight="17.0" prefWidth="200.0" textFill="#fc8585" />
      <Label fx:id="lastnameLabel" alignment="CENTER" layoutX="250.0" layoutY="191.0" prefHeight="17.0" prefWidth="200.0" textFill="#fc8585" />
      <Label fx:id="usernameLabel" alignment="CENTER" layoutX="250.0" layoutY="251.0" prefHeight="17.0" prefWidth="200.0" textFill="#fc8585" />
      <Label fx:id="emailLabel" alignment="CENTER" layoutX="250.0" layoutY="312.0" prefHeight="17.0" prefWidth="200.0" textFill="#fc8585" />
   </children>
</AnchorPane>
